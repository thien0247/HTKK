															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
												<xs:element name="nguoiKy" type="xs:string" nillable="true">
													<xs:annotation>
														<xs:documentation>Người ký</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="ngayKy" type="xs:date">
