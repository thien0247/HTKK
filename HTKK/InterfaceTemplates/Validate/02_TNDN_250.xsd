													</xs:annotation>
												</xs:element>
												<xs:element name="ngayXN" type="xs:date" nillable="true">
													<xs:annotation>
														<xs:documentation>ngày xác nhận</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="ct25" type="xs:nonNegativeInteger" nillable="true">
							<xs:annotation>
