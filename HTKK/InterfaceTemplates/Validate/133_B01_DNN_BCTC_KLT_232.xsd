													</xs:annotation>
												</xs:element>
												<xs:element name="ct60" type="xs:long" nillable="true">
													<xs:annotation>
														<xs:documentation>Tiền và tương đương tiền đầu kỳ</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="ct61" type="xs:long" nillable="true">
													<xs:annotation>
														<xs:documentation>Ảnh hưởng của thay đổi tỷ giá hối đoái quy đổi ngoại tệ</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="ct70" type="xs:long" nillable="true">
													<xs:annotation>
														<xs:documentation>Tiền và tương đương tiền cuối kỳ (70 = 50+60+61)</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
