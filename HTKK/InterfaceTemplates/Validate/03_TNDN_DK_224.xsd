<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://kekhaithue.gdt.gov.vn/TKhaiThue" targetNamespace="http://kekhaithue.gdt.gov.vn/TKhaiThue" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.0.0">
	<xs:redefine schemaLocation="../TKhaiThue.xsd">
		<xs:complexType name="CTieuTKhaiChinhType">
			<xs:complexContent>
				<xs:extension base="CTieuTKhaiChinhType">
					<xs:sequence>
						<xs:element name="Header">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="BenChuyenbNhuong">
										<xs:annotation>
											<xs:documentation>Bên chuyển nhượng quyền lợi:</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="ct05" type="xs:string" nillable="true">
													<xs:annotation>
														<xs:documentation>Tên tổ chức chuyển nhượng:</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="ct06" type="TINType" nillable="true">
													<xs:annotation>
														<xs:documentation> Mã số thuế (nếu có): </xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="ct07" type="xs:string" nillable="true">
													<xs:annotation>
														<xs:documentation>Địa chỉ trụ sở chính:  </xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="ct08" type="xs:string" nillable="true">
													<xs:annotation>
														<xs:documentation>Hợp đồng dầu khí lô:</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="ct09_so" type="xs:string" nillable="true">
													<xs:annotation>
														<xs:documentation>Hợp đồng chuyển nhượng số.....</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="ct09_ngay" type="xs:date" nillable="true">
													<xs:annotation>
														<xs:documentation>ngày......</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="BenNhanChuyenNhuong">
										<xs:annotation>
											<xs:documentation>Bên nhận chuyển nhượng:</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="ct10" type="xs:string" nillable="true">
													<xs:annotation>
														<xs:documentation>Tên tổ chức hoặc cá nhân nhận chuyển nhượng:</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="ct11" type="xs:string" nillable="true">
													<xs:annotation>
														<xs:documentation> Địa chỉ trụ sở chính:   </xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="ct12" type="TINType" nillable="true">
													<xs:annotation>
														<xs:documentation> Mã số thuế (đối với doanh nghiệp):</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="ct13" type="xs:string" nillable="true">
													<xs:annotation>
														<xs:documentation> Mã số thuế hoặc số chứng minh nhân dân, số hộ chiếu (đối với cá nhân):</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
